        "# 예측값과 실제값 차이 (잔차) 계산\n",
        "errors = y_valid.values - y_pred  # 잔차 = 실제값 - 예측값\n",
        "\n",
        "# DataFrame으로 정리\n",
        "results_df = pd.DataFrame({\n",
        "    '실제 운임': y_valid.values,\n",
        "    '예측 운임': y_pred,\n",
        "    '오차 (실제 - 예측)': errors})\n"
